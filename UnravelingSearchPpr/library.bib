journal = {Demography},
number = {1},
pages = {1--22},
pmid = {15782893},
title = {{The role of public health improvements in health advances: the twentieth-century United States.}},
volume = {42},
year = {2005}
}
@article{Finkelstein2012,
abstract = {In 2008, a group of uninsured low-income adults in Oregon was selected by lottery to be given the chance to apply for Medicaid. This lottery provides an opportunity to gauge the effects of expanding access to public health insurance on the health care use, financial strain, and health of low-income adults using a randomized controlled design. In the year after random assignment, the treat- ment group selected by the lottery was about 25 percentage points more likely to have insurance than the control group that was not selected. We find that in this first year, the treatment group had substantively and statistically signifi- cantly higher health care utilization (including primary and preventive care as well as hospitalizations), lower out-of-pocket medical expenditures and medical debt (including fewer bills sent to collection), and better self-reported physical and mental health than the control group. JEL Codes: H51, H75, I1.},
annote = {Random assignment to Medicaid access in 2008, Treatment group had 25{\%} more health insurance. 
Time under treatment was about 14 months, 29,834  of 74,922 individuals were offered Medicaid. 
Treatment group was low-income adults, randomly selected off of waitlist. 
20-64 years, mostly younger (73{\%} 20-50), 82{\%} white, 3.8{\%} black, 12{\%} Spanish/hispanic/latino 
  
18{\%} hs dropout, 49{\%} hs diploma/ged, balance had some vocational or 4 year degree (or more) 
  
55{\%} unemployed, 26{\%} 30+ hs/wk, average income 13k 
  
Results:  PP change ({\%} of baseline change) 
Using instrument method: insurance gives 2.1(30){\%} increase in likelihood of hospital admission, 8.8(15) increase in probability of taking any drugs, 21(35) increase in having at least 1 outpatient visit.  
  
Can't rule out effects on ER visits on the order of 5-10 (10-20){\%} in either direction, though in light of evidence ER visits look like they go up. 
  
  
Increased use of preventative care (eg, mammograms, cholesterol checks), .3 SD 
  
6.4(25){\%} drop in likelihood of a medical bill sent to collections, 20(35{\%}) drop in chance of having any out of poket expenses.  Note that medical bills sent to collections are rarely actually paid, the incidence of cost of uninsured care is also on provider (and presumably eventually on the insured patients) 
  
Significant subjective improvemtn of health outcomes 2/2 enrollment in Medicaid.},
author = {Finkelstein, Amy and Taubman, Sarah and Wright, Bill and Bernstein, Mira and Gruber, Jonathan and Newhouse, Joseph P. and Allen, Heidi and Baicker, Katherine},
doi = {10.1093/qje/qjs020},
isbn = {0033-5533},
issn = {00335533},
journal = {Quarterly Journal of Economics},
pmid = {23293397},
title = {{The oregon health insurance experiment: Evidence from the first year}},
year = {2012}
}
@article{McKellar2014,
abstract = {OBJECTIVE: To examine the relationship between insurance market structure and health care prices, utilization, and spending. DATA SOURCES: Claims for 37.6 million privately insured employees and their dependents from the Truven Health Market Scan Database in 2009. Measures of insurer market structure derived from Health Leaders Inter study data. METHODS: Regression models are used to estimate the association between insurance market concentration and health care spending, utilization, and price, adjusting for differences in patient characteristics and other market-level traits. RESULTS: Insurance market concentration is inversely related to prices and spending, but positively related to utilization. Our results imply that, after adjusting for input price differences, a market with two equal size insurers is associated with 3.9 percent lower medical care spending per capita (p = .002) and 5.0 percent lower prices for health care services relative to one with three equal size insurers (p {\textless} .001). CONCLUSION: Greater fragmentation in the insurance market might lead to higher prices and higher spending for care, suggesting some of the gains from insurer competition may be absorbed by higher prices for health care. Greater attention to prices and utilization in the provider market may need to accompany procompetitive insurance market strategies.},
author = {McKellar, Michael R. and Naimer, Sivia and Landrum, Mary B. and Gibson, Teresa B. and Chandra, Amitabh and Chernew, Michael},
doi = {10.1111/1475-6773.12131},

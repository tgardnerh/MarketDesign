pages = {81--106},
title = {{The Medicare Cost Report and the Limits of Hospital Accountability: Improving Financial Accounting Data}},
url = {http://jhppl.dukejournals.org.ezp-prod1.hul.harvard.edu/content/26/1/81.short},
volume = {26},
year = {2001}
}

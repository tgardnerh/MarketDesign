keywords = {Area Under Curve,Body Mass Index,Breast Neoplasms,Breast Neoplasms: epidemiology,Breast Neoplasms: etiology,Case-Control Studies,Female,Humans,Iran,Middle Aged,Obesity,Obesity: complications,Obesity: epidemiology,ROC Curve,Risk Factors,Waist Circumference,Waist Circumference: physiology},
month = {dec},
number = {4},
pages = {1296--301},
pmid = {20661665},
title = {{Body mass index and waist circumference are predictor biomarkers of breast cancer risk in Iranian women.}},

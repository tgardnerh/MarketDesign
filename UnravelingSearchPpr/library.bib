@article{Barnato2005,
abstract = {BACKGROUND: Previous studies have documented racial disparities in treatment of acute myocardial infarction (AMI) among Medicare beneficiaries. However, the extent to which unobserved differences between hospitals explains some of these differences is unknown. OBJECTIVE: The objective of this study was to determine whether the observed racial treatment disparities for AMI narrow when analyses account for differences in where blacks and whites are hospitalized. RESEARCH DESIGN: Retrospective observational cohort study using Medicare claims and medical record review. SUBJECTS: This study included 130,709 white and 8286 black Medicare patients treated in 4690 hospitals in 50 US states for confirmed AMI in 1994 and 1995. MEASURES: Measures in this study were receipt of reperfusion, aspirin, and smoking cessation counseling during hospitalization; prescription of aspirin, angiotensin-converting enzyme inhibitor, and beta-blocker at hospital discharge; receipt of cardiac catheterization, percutaneous coronary intervention (PCI), or bypass surgery (CABG) within 30 days of AMI; and 30-day and 1-year mortality. RESULTS: Within-hospital analyses narrowed or erased black-white disparities for medical treatments received during the acute hospitalization, widened black-white disparities for follow-up surgical treatments, and augmented the survival advantage among blacks. These findings indicate that, on average, blacks went to hospitals that had lower rates of evidence-based medical treatments, higher rates of cardiac procedures, and worse risk-adjusted mortality after AMI. CONCLUSIONS: Incorporating the hospital effect altered the findings of racial disparity analyses in AMI and explained more of the disparities than race. A policy of targeted hospital-level interventions may be required for success of national efforts to reduce disparities.},
author = {Barnato, Amber E and Lucas, F Lee and Staiger, Douglas and Wennberg, David E and Chandra, Amitabh},
doi = {10.1097/01.mlr.0000156848.62086.06},
file = {:Users/tylerhoppenfeld/Library/Application Support/Mendeley Desktop/Downloaded/Barnato et al. - 2005 - Hospital-level racial disparities in acute myocardial infarction treatment and outcomes.pdf:pdf},
issn = {0025-7079},
journal = {Medical care},
keywords = {disparities,ethnicity,hospital quality,mortality,myocardial infarction,race},
number = {4},
pages = {308--319},
pmid = {15778634},
title = {{Hospital-level racial disparities in acute myocardial infarction treatment and outcomes.}},
volume = {43},
year = {2005}
}

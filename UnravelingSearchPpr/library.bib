issn = {14756773},
journal = {Health Services Research},
keywords = {Geographic variation,competition,markets,spending,utilization},
pages = {878--892},
pmid = {24303879},
title = {{Insurer market structure and variation in commercial health care spending}},
volume = {49},
year = {2014}
}
